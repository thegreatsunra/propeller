    <![endif]-->
  </head>

  <body>

    {{> navbar }}

    <div class="container">
      {{> body }}
    </div><!-- /.container -->

    <!-- All JavaScript via RequireJS -->
    <script data-main="{{site.jsFolder}}/require.config.js" src="{{site.componentsFolder}}/requirejs/require.js"></script>

    {{> scripts }}
